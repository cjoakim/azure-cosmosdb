{
    "indexingMode": "consistent",
    "automatic": true,
    "includedPaths": [
        {
            "path": "/*"
        }
    ],
    "excludedPaths": [
        {
            "path": "/\"_etag\"/?"
        }
    ],
    "compositeIndexes": [
        [
            {
                "path": "/accountNumber",
                "order": "descending"
            },
            {
                "path": "/type",
                "order": "descending"
            },
            {
                "path": "/addressType",
                "order": "descending"
            }
        ],
        [
            {
                "path": "/_ts",
                "order": "descending"
            },
            {
                "path": "/accountNumber",
                "order": "ascending"
            }
        ]
    ]
}
